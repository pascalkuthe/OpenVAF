          T0 = exp(BSIM3vjdmFwd / Nvtm);
          BSIM3IVjdmFwd = BSIM3DrainSatCurrent * (T0 - BSIM3XExpBVD / T0
            		  + BSIM3XExpBVD - 1.0);
          BSIM3DslpFwd = BSIM3DrainSatCurrent
                              * (T0 + BSIM3XExpBVD / T0) / Nvtm;

