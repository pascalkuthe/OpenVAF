            BSIM3drainSquares = 0.0;
        
        if (nrs  == `NOT_GIVEN) 
            BSIM3sourceSquares = 0.0;
        
          arg = wmlt * xmult;
          Leff = l*lmlt+xl-(ld+del)*2.0;
          Weff = w*wmlt+xw*2.0;
          here_w=Weff-wd*2.0;
          if (here_w == 0.0)
            here_w = 1.0e-12;

          rseff = rs;
          rdeff = rd;            
          if (wrs != 0.0 || lrs != 0.0 || prs != 0.0 || wrd != 0.0 
