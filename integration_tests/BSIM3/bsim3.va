          if ((BSIM3drainArea <= 0.0) && (BSIM3drainPerimeter <= 0.0))
              BSIM3DrainSatCurrent = 1.0e-14;
          else
              BSIM3DrainSatCurrent = BSIM3drainArea
                                   * BSIM3jctTempSatCurDensity
                                   + BSIM3drainPerimeter
                                   * BSIM3jctSidewallTempSatCurDensity;
