      	   T2 = 1.0 / (Leff * 1e6) - T0;
      	   T3 = 1.0 / (here_w * 1e6) - T1;
      	   T4 = T2 * T3;
      	   rseff = rs + wrs * T3 + lrs * T2 + prs * T4;
